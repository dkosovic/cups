summary: CI plan, picks internal Tier1 tests, runs in beakerlib.
discover:
  - name: rhel
    how: fmf
    filter: 'tier: 1'
    url: git://pkgs.devel.redhat.com/tests/cups
execute:
    how: tmt
adjust:
    enabled: false
    when: distro == centos-stream-* or distro == fedora-*
    because: They don't have access to internal repos.
